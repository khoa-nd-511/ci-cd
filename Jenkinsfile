pipeline {

    agent any

    stages {

        stage("test") {
            
            steps {
                echo "Executing tests..."
                echo "Tests executed successfully ⚙️︎"
            }
        }

        stage("build") {
            
            steps {
                echo "Building app..."
                echo "App is built successfully 💪"
            }
        }

        stage("deploy") {
            
            steps {
                echo "Deploying..."
                echo "App is deployed successfully 🚀"
            }
        }
    }
}